<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                                   xmlns:app="http://schemas.android.com/apk/res-auto"
                                                   xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
                                                   android:layout_height="match_parent">
    <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@drawable/limeta_blurred"
            android:id="@+id/linear_layout"
            android:orientation="vertical">
    </LinearLayout>
    <Button
            android:text="NEXT"
            android:layout_width="96dp"
            android:layout_height="55dp" android:id="@+id/button24"
            app:backgroundTint="#4C9E45" android:textSize="20sp"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="30dp" android:onClick="next10"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginRight="30dp"
            android:layout_marginEnd="30dp"/>
    <Button
            android:text="BACK"
            android:layout_width="96dp"
            android:layout_height="55dp" android:id="@+id/button23"
            app:backgroundTint="#4C9E45" android:textSize="20sp"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="30dp" android:onClick="back8"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="30dp"
            android:layout_marginStart="30dp"/>
    <LinearLayout
            android:orientation="horizontal"
            android:layout_width="404dp"
            android:layout_height="317dp"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="70dp"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
            android:id="@+id/linearLayout4">
        <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="346dp">
            <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="55dp"
                    android:layout_height="348dp">
            </LinearLayout>
            <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="294dp"
                    android:layout_height="348dp">
                <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="319dp" app:srcCompat="@drawable/info_profile_statistics"
                        android:id="@+id/imageView28"/>
            </LinearLayout>
            <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="65dp"
                    android:layout_height="349dp">
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
    <LinearLayout
            android:orientation="horizontal"
            android:layout_width="370dp"
            android:layout_height="169dp"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/linearLayout4" android:layout_marginTop="30dp"
            app:layout_constraintBottom_toBottomOf="@+id/linear_layout" android:layout_marginBottom="125dp"
            android:layout_marginLeft="20dp" android:layout_marginStart="20dp" android:layout_marginRight="20dp"
            android:layout_marginEnd="20dp">
        <TextView
                android:text="These are your statistics: consumed calories and water on a daily basis. Moreover, Healthy Diet provides you with weekly and monthly insights."
                android:layout_width="wrap_content"
                android:layout_height="match_parent" android:id="@+id/textView13" android:layout_weight="1"
                android:gravity="center" android:textColor="#517528" android:textSize="23sp"/>
    </LinearLayout>
    <TextView
            android:text="X"
            android:layout_width="19dp"
            android:layout_height="36dp" android:id="@+id/textView8"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintEnd_toEndOf="parent"
            android:textSize="30sp" android:layout_marginTop="20dp" android:layout_marginRight="20dp"
            android:layout_marginEnd="20dp" android:textColor="#517528" android:textStyle="bold"
            android:onClick="close9"/>
</androidx.constraintlayout.widget.ConstraintLayout>