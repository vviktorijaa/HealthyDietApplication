                    android:layout_width="match_parent"
                    android:layout_height="45dp">
                <Switch
                        android:id="@+id/switchLowCalorie"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"/>
            </LinearLayout>
            <LinearLayout
