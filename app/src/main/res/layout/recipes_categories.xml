                    android:layout_width="match_parent"
                    android:layout_height="12dp">

            </LinearLayout>
            <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="45dp">
                <Switch
                        android:id="@+id/switchHighCalorie"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"/>
            </LinearLayout>
            <LinearLayout
