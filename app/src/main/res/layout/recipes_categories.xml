            android:layout_width="130dp"
            android:layout_height="wrap_content" android:id="@+id/textView29"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintTop_toBottomOf="@+id/imageView37"
            app:layout_constraintStart_toStartOf="parent" android:gravity="center" android:textColor="#DEDFBE"
            android:textSize="20sp" android:layout_marginBottom="8dp"
            app:layout_constraintEnd_toEndOf="@+id/linearLayout14"/>
    <ImageView
            android:layout_width="68dp"
            android:layout_height="53dp" app:srcCompat="@drawable/my_profile_kremovo"
            android:id="@+id/imageView48"
            app:layout_constraintBottom_toBottomOf="@+id/linear_layout"
            app:layout_constraintEnd_toEndOf="@+id/linearLayout14" app:layout_constraintStart_toStartOf="parent"
