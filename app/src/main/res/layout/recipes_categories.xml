    <TextView
            android:text="Home"
            android:layout_width="130dp"
            android:layout_height="wrap_content" android:id="@+id/textView27"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintTop_toBottomOf="@+id/imageView37"
            app:layout_constraintStart_toStartOf="parent" android:gravity="center" android:textColor="#DEDFBE"
            android:textSize="20sp" android:layout_marginBottom="8dp"/>
    <ImageView
            android:layout_width="68dp"
            android:layout_height="53dp" app:srcCompat="@drawable/home_kremovo" android:id="@+id/imageView37"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginBottom="30dp" android:layout_marginLeft="30dp" android:layout_marginStart="30dp"/>
    <ImageView
            android:layout_width="41dp"
            android:layout_height="43dp" app:srcCompat="@drawable/back" android:id="@+id/imageView32"
            android:onClick="backToRecipesActivity" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="30dp" android:layout_marginLeft="30dp" android:layout_marginStart="30dp"/>
</androidx.constraintlayout.widget.ConstraintLayout>