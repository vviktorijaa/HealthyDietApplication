<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                                   xmlns:app="http://schemas.android.com/apk/res-auto"
                                                   xmlns:tools="http://schemas.android.com/tools"
                                                   android:layout_width="match_parent"
                                                   android:layout_height="match_parent"
>
    <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@drawable/apple_blurred"
            android:id="@+id/linear_layout"
            android:orientation="vertical" tools:layout_editor_absoluteY="0dp" tools:layout_editor_absoluteX="0dp">
    </LinearLayout>
    <LinearLayout
            android:orientation="horizontal"
            android:layout_width="414dp"
            android:layout_height="86dp"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent" android:id="@+id/linearLayout14">
        <LinearLayout
                android:orientation="vertical"
                android:layout_width="137dp"
                android:layout_height="match_parent" android:onClick="home">
            <TextView
                    android:layout_width="134dp"
                    android:layout_height="match_parent" android:id="@+id/homeMenu6"
                    android:background="@drawable/rectangle"
                    app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toStartOf="@+id/profileMenu"
                    app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintHorizontal_bias="0.0"
                    android:onClick="home"/>
        </LinearLayout>
        <LinearLayout
                android:orientation="vertical"
                android:layout_width="141dp"
                android:layout_height="match_parent" android:onClick="profile">
            <TextView
                    android:layout_width="137dp"
                    android:layout_height="match_parent" android:id="@+id/profileMenu7"
                    android:background="@drawable/rectangle2"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent" app:layout_constraintBottom_toBottomOf="parent"
                    android:onClick="profile"/>
        </LinearLayout>
        <LinearLayout
                android:orientation="vertical"
                android:layout_width="137dp"
                android:layout_height="match_parent" android:onClick="recipes">
            <TextView
                    android:layout_width="134dp"
                    android:layout_height="match_parent" android:id="@+id/recipesMenu6"
                    android:background="@drawable/rectangle3"
