                        android:layout_width="wrap_content"
                        android:layout_height="match_parent" android:id="@+id/textView100" android:layout_weight="1"
                        android:textColor="#517528" android:textSize="27sp" android:gravity="center|start"/>
            </LinearLayout>
            <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="12dp">
