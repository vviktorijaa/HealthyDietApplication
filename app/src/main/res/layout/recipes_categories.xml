                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" android:id="@+id/textView94" android:layout_weight="1"
                        android:textColor="#517528" android:textStyle="bold"/>
            </LinearLayout>
            <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="45dp">
                <TextView
                        android:text="Snacks"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent" android:id="@+id/textView97" android:layout_weight="1"
                        android:textColor="#517528" android:textSize="27sp" android:gravity="center|start"/>
            </LinearLayout>
            <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="12dp">
                <TextView
                        android:text="────────────────────────────────────"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" android:id="@+id/textView96" android:layout_weight="1"
                        android:textColor="#517528" android:textStyle="bold"/>
            </LinearLayout>
            <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
